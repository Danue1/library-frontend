{
  "presets": [
    ["next/babel",
      {
        "preset-env": {
          "useBuiltIns": "usage"
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-optional-chaining"],
  "env": {
    "production": {
      "plugins": [
        [
          "emotion", {
            "hoist": true
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "emotion", {
            "sourceMap": true,
            "autoLabel": true
          }
        ]
      ]
    }
  }
}